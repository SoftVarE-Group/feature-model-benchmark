CYGPKG_HAL ? --package
 -- eCos HAL
 [CYGPKG_INFRA]
 CYGPKG_HAL_COMMON --component
  -- Platform-independent HAL options
  CYGFUN_HAL_COMMON_KERNEL_SUPPORT ? --option
   -- Provide eCos kernel support
   [CYGPKG_KERNEL]
  CYGPKG_HAL_EXCEPTIONS ? --option
   -- HAL exception support
   [CYGPKG_KERNEL_EXCEPTIONS]
  CYGSEM_HAL_STOP_CONSTRUCTORS_ON_FLAG ? --option
   -- Stop calling constructors early
   [CYGSEM_LIBC_INVOKE_DEFAULT_STATIC_CONSTRUCTORS]
  CYGINT_HAL_SUPPORTS_MMU_TABLES -> int --interface
   -- HAL uses the MMU and allows for CDL manipulation of it's use
   [this == #CYGINT_HAL_SUPPORTS_MMU_TABLES_ABSTRACT]
  CYGSEM_HAL_INSTALL_MMU_TABLES ? --option
   -- Install MMU tables.
   [(~((CYGINT_HAL_SUPPORTS_MMU_TABLES => CYGINT_HAL_SUPPORTS_MMU_TABLES else 0) == 0))]
  CYGSEM_HAL_STATIC_MMU_TABLES ? --option
   -- Use static MMU tables.
   [CYGSEM_HAL_INSTALL_MMU_TABLES]
  CYGDBG_HAL_DIAG_TO_DEBUG_CHAN ? --component
   -- Route diagnostic output to debug channel
   [(~CYGSEM_HAL_VIRTUAL_VECTOR_INHERIT_CONSOLE)]
   [((false || false) || false) || CYGSEM_HAL_VIRTUAL_VECTOR_DIAG]
   CYGSEM_HAL_DIAG_MANGLER -> string --option
    -- Mangler used on diag output
    [(this == "GDB") || (this == "None")]
  CYGBLD_HAL_LINKER_GROUPED_LIBS -> string ? --component
   -- Grouped libraries for linking
   [isSubstring(this , "libtarget.a")]
   CYGBLD_HAL_LINKER_GROUPED_LIBS_DEFAULT -> string --option
    -- Default setting
 CYGPKG_HAL_COMMON_INTERRUPTS --component
  -- HAL interrupt handling
  CYGIMP_HAL_COMMON_INTERRUPTS_USE_INTERRUPT_STACK ? --option
   -- Use separate stack for interrupts
  CYGNUM_HAL_COMMON_INTERRUPTS_STACK_SIZE -> int --option
   -- Interrupt stack size
   [(this >= 128) && (this <= 1048576)]
  CYGSEM_HAL_COMMON_INTERRUPTS_ALLOW_NESTING ? --option
   -- Allow nested interrupts
  CYGDBG_HAL_COMMON_INTERRUPTS_SAVE_MINIMUM_CONTEXT ? --option
   -- Save minimum context on interrupt
  CYGIMP_HAL_COMMON_INTERRUPTS_CHAIN ? --option
   -- Chain all interrupts together
  CYGIMP_HAL_COMMON_INTERRUPTS_IGNORE_SPURIOUS ? --option
   -- Ignore spurious [fleeting] interrupts
 CYGPKG_HAL_COMMON_CONTEXT --component
  -- HAL context switch support
  CYGDBG_HAL_COMMON_CONTEXT_SAVE_MINIMUM ? --option
   -- Use minimum thread context
 CYGPKG_HAL_CACHE_CONTROL --component
  -- Explicit control over cache behaviour
  CYGSEM_HAL_ENABLE_DCACHE_ON_STARTUP ? --component
   -- Enable DATA cache on startup
   CYGSEM_HAL_DCACHE_STARTUP_MODE -> string --option
    -- DATA cache mode on startup
    [(this == "COPYBACK") || (this == "WRITETHRU")]
  CYGSEM_HAL_ENABLE_ICACHE_ON_STARTUP ? --option
   -- Enable INSTRUCTION cache on startup
 CYGPKG_HAL_DEBUG --component
  -- Source-level debugging support
  CYGINT_HAL_DEBUG_GDB_STUBS -> int --interface
   -- Support for GDB stubs
   [this == #CYGINT_HAL_DEBUG_GDB_STUBS_ABSTRACT]
  CYGDBG_HAL_DEBUG_GDB_INCLUDE_STUBS ? --option
   -- Include GDB stubs in HAL
   [(~CYGSEM_HAL_USE_ROM_MONITOR)]
   [(~CYGDBG_HAL_COMMON_INTERRUPTS_SAVE_MINIMUM_CONTEXT)]
   [(~CYGDBG_HAL_COMMON_CONTEXT_SAVE_MINIMUM)]
   [(~CYGSEM_HAL_VIRTUAL_VECTOR_DIAG) || CYGSEM_HAL_VIRTUAL_VECTOR_CLAIM_COMMS]
   [(~((CYGINT_HAL_DEBUG_GDB_STUBS => CYGINT_HAL_DEBUG_GDB_STUBS else 0) == 0))]
  CYGINT_HAL_DEBUG_GDB_STUBS_BREAK -> int --interface
   -- Support for external break support in GDB stubs
   [this == #CYGINT_HAL_DEBUG_GDB_STUBS_BREAK_ABSTRACT]
  CYGDBG_HAL_DEBUG_GDB_BREAK_SUPPORT ? --option
   -- Include GDB external break support for stubs
   [CYGDBG_HAL_DEBUG_GDB_INCLUDE_STUBS]
   [(~((CYGINT_HAL_DEBUG_GDB_STUBS_BREAK => CYGINT_HAL_DEBUG_GDB_STUBS_BREAK else 0) == 0))]
  CYGINT_HAL_DEBUG_GDB_CTRLC_UNSUPPORTED -> int --interface
   -- Platform does not support CTRLC
   [this == #CYGINT_HAL_DEBUG_GDB_CTRLC_UNSUPPORTED_ABSTRACT]
  CYGDBG_HAL_DEBUG_GDB_CTRLC_SUPPORT ? --option
   -- Include GDB external break support when no stubs
   [(~CYGDBG_HAL_DEBUG_GDB_BREAK_SUPPORT)]
   [CYGSEM_HAL_USE_ROM_MONITOR || CYGDBG_HAL_DEBUG_GDB_INCLUDE_STUBS]
   [(CYGINT_HAL_DEBUG_GDB_CTRLC_UNSUPPORTED => CYGINT_HAL_DEBUG_GDB_CTRLC_UNSUPPORTED else 0) == 0]
  CYGDBG_HAL_DEBUG_GDB_THREAD_SUPPORT ? --option
   -- Include GDB multi-threading debug support
   [CYGSEM_HAL_ROM_MONITOR || CYGDBG_KERNEL_DEBUG_GDB_THREAD_SUPPORT]
  CYGNUM_HAL_DEBUG_GDB_PROTOCOL_RETRIES -> string --option
   -- Number of times to retry sending a $O packet
  CYGNUM_HAL_DEBUG_GDB_PROTOCOL_TIMEOUT -> string --option
   -- Timeout period for GDB packets
  CYGDBG_HAL_CRCTABLE_LOCATION -> string --option
   -- Location of CRC32 table
   [(this == "ROM") || (this == "RAM")]
 CYGPKG_HAL_ROM_MONITOR --component
  -- ROM monitor support
  CYGINT_HAL_VIRTUAL_VECTOR_SUPPORT -> int --interface
   -- Target has virtual vector support
   [this == #CYGINT_HAL_VIRTUAL_VECTOR_SUPPORT_ABSTRACT]
  CYGINT_HAL_VIRTUAL_VECTOR_COMM_BAUD_SUPPORT -> int --interface
   -- Target supports baud rate control via vectors
   [this == #CYGINT_HAL_VIRTUAL_VECTOR_COMM_BAUD_SUPPORT_ABSTRACT]
  CYGSEM_HAL_VIRTUAL_VECTOR_SUPPORT ? --component
   -- Enable use of virtual vector calling interface
   [(~((CYGINT_HAL_VIRTUAL_VECTOR_SUPPORT => CYGINT_HAL_VIRTUAL_VECTOR_SUPPORT else 0) == 0))]
   CYGSEM_HAL_VIRTUAL_VECTOR_INHERIT_CONSOLE ? --option
    -- Inherit console settings from ROM monitor
    [CYGSEM_HAL_USE_ROM_MONITOR]
   CYGPRI_HAL_VIRTUAL_VECTOR_DEBUG_CHANNEL_CONFIGURABLE ? --option
    -- Debug channel is configurable
   CYGPRI_HAL_VIRTUAL_VECTOR_CONSOLE_CHANNEL_CONFIGURABLE ? --option
    -- Console channel is configurable
   CYGSEM_HAL_VIRTUAL_VECTOR_INIT_WHOLE_TABLE ? --option
    -- Initialize whole of virtual vector table
    [CYGSEM_HAL_VIRTUAL_VECTOR_CLAIM_RESET]
    [CYGSEM_HAL_VIRTUAL_VECTOR_CLAIM_DELAY_US]
    [CYGSEM_HAL_VIRTUAL_VECTOR_CLAIM_DATA]
    [CYGSEM_HAL_VIRTUAL_VECTOR_CLAIM_COMMS]
   CYGSEM_HAL_VIRTUAL_VECTOR_CLAIM_DEFAULT ? --option
    -- Claim virtual vector table entries by default
    [(~CYGSEM_HAL_VIRTUAL_VECTOR_INIT_WHOLE_TABLE)]
   CYGSEM_HAL_VIRTUAL_VECTOR_CLAIM_RESET ? --option
    -- Claim reset virtual vectors
   CYGSEM_HAL_VIRTUAL_VECTOR_CLAIM_VERSION ? --option
    -- Claim version virtual vectors
   CYGSEM_HAL_VIRTUAL_VECTOR_CLAIM_DELAY_US ? --option
    -- Claim delay_us virtual vector
   CYGSEM_HAL_VIRTUAL_VECTOR_CLAIM_DATA ? --option
    -- Claim data virtual vectors
   CYGSEM_HAL_VIRTUAL_VECTOR_CLAIM_COMMS ? --option
    -- Claim comms virtual vectors
   CYGSEM_HAL_VIRTUAL_VECTOR_DIAG ? --option
    -- Do diagnostic IO via virtual vector table
  CYGSEM_HAL_USE_ROM_MONITOR ? --option
   -- Work with a ROM monitor
   [(CYG_HAL_STARTUP => CYG_HAL_STARTUP else "0") == "RAM"]
   [(~CYGSEM_HAL_POWERPC_COPY_VECTORS)]
   [(~CYGDBG_HAL_DEBUG_GDB_INCLUDE_STUBS)]
   [(~(~((CYGINT_HAL_USE_ROM_MONITOR_UNSUPPORTED => CYGINT_HAL_USE_ROM_MONITOR_UNSUPPORTED else 0) == 0)))]
  CYGSEM_HAL_ROM_MONITOR ? --option
   -- Behave as a ROM monitor
   [(CYG_HAL_STARTUP => CYG_HAL_STARTUP else "0") == "ROM"]
 CYGINT_HAL_PLF_IF_INIT -> int --interface
  -- Platform defined I/O channels.
  [this == #CYGINT_HAL_PLF_IF_INIT_ABSTRACT]
 CYGINT_HAL_PLF_IF_IDE -> int --interface
  -- Platform IDE I/O support.
  [this == #CYGINT_HAL_PLF_IF_IDE_ABSTRACT]
 CYGPKG_HAL_GDB_FILEIO ? --option
  -- File I/O operations via GDB
  [CYGDBG_HAL_DEBUG_GDB_INCLUDE_STUBS]
  [false]
 CYGPKG_HAL_BUILD_COMPILER_TESTS ? --option
  -- Build Compiler sanity checking tests
 CYGPKG_HAL_TESTS -> string --component
  -- Common HAL tests
  [this == ((("tests/context tests/basic" ++ ((~(~((CYGINT_HAL_TESTS_NO_CACHES => CYGINT_HAL_TESTS_NO_CACHES else 0) == 0))) => " tests/cache" else "")) ++ (CYGPKG_HAL_BUILD_COMPILER_TESTS => " tests/cpp1 tests/vaargs" else "")) ++ ((~CYGVAR_KERNEL_COUNTERS_CLOCK) => " tests/intr" else ""))]
  CYGINT_HAL_TESTS_NO_CACHES -> int ? --interface
   -- Interface for cache presence
   [this == #CYGINT_HAL_TESTS_NO_CACHES_ABSTRACT]
   [#CYGINT_HAL_TESTS_NO_CACHES_ABSTRACT > 0]
 CYGPKG_HAL_POWERPC ? --package
  -- PowerPC architecture
  CYGINT_HAL_POWERPC_VARIANT -> int --interface
   -- Number of variant implementations in this configuration
   [this == #CYGINT_HAL_POWERPC_VARIANT_ABSTRACT]
   [1 == this]
  CYGSEM_HAL_POWERPC_RESET_USES_JUMP ? --option
   -- RESET vector jumps to startup
  CYGSEM_HAL_POWERPC_COPY_VECTORS ? --option
   -- Copy exception vectors to RAM
   [(~CYGSEM_HAL_USE_ROM_MONITOR)]
  CYGHWR_HAL_POWERPC_NEED_VECTORS ? --option
   -- Exception vectors inclusion
  CYGHWR_HAL_POWERPC_VECTOR_BASE -> int --option
   -- Exception vectors location
   [this == (((~CYGHWR_HAL_POWERPC_FORCE_VECTOR_BASE_LOW) && (false || ((~((CYG_HAL_STARTUP => CYG_HAL_STARTUP else "0") == "RAM")) && (~CYGSEM_HAL_POWERPC_COPY_VECTORS)))) => 4293918720 else 0)]
  CYGHWR_HAL_POWERPC_ENABLE_MMU ? --option
   -- Enable MMU
  CYGDBG_HAL_POWERPC_FRAME_WALLS ? --option
   -- Exception stack-frame walls
  CYGSEM_REDBOOT_HAL_LINUX_BOOT ? --option
   -- Support booting Linux via RedBoot
   [0 == 1]
  CYGPKG_HAL_POWERPC_OPTIONS --component
   -- PowerPC build options
   CYGPKG_HAL_POWERPC_CFLAGS_ADD -> string --option
    -- Additional compiler flags
   CYGPKG_HAL_POWERPC_CFLAGS_REMOVE -> string --option
    -- Suppressed compiler flags
   CYGPKG_HAL_POWERPC_TESTS -> string --option
    -- PowerPC tests
    [this == ""]
  CYGBLD_LINKER_SCRIPT -> string --option
   -- Linker script
   [this == "src/powerpc.ld"]
  CYGPKG_HAL_POWERPC_MPC5xx ? --package
   -- PowerPC 5xx variant HAL
   CYGINT_HAL_USE_ROM_MONITOR_UNSUPPORTED -> int --interface
    -- ROM monitor configuration is unsupported
    [this == #CYGINT_HAL_USE_ROM_MONITOR_UNSUPPORTED_ABSTRACT]
   CYGSEM_HAL_USE_ROM_MONITOR_GDB_stubs ? --option
    -- Bad CDL workaround
    [CYGSEM_HAL_USE_ROM_MONITOR]
   CYGPKG_HAL_POWERPC_MPC555 ? --component
